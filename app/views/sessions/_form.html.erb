<%= form_with url: sessions_path, local: true,  data: { turbo: false } do |form| %>

  <% if flash[:alert] %>
    <%= render "shared/flash" %>
  <% end %>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email, id: :email %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password, id: :password %>
  </p>

  <p><%= form.submit 'Enter' %></p>
<% end %>
